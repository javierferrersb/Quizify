<Window x:Class="QuizifyUI.Pesos.AsignarCompetencia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizifyUI.Pesos"
        mc:Ignorable="d"
        Title="AsignarCompetencia" Height="350" Width="450">
    <Grid>
        <Button x:Name="saveCompetencia" Content="Guardar" HorizontalAlignment="Left" Margin="260,254,0,0"  VerticalAlignment="Top" RenderTransformOrigin="0.489,-0.036" Width="94" Click="save_click" Cursor="Hand"/>
        <Button x:Name="exitCompetencia" Content="Salir" HorizontalAlignment="Left" Margin="96,254,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.489,-0.036" Width="94" Click="exit_Click" Cursor="Hand"/>
        <TextBox x:Name="markBox" HorizontalAlignment="Left" Margin="225,131,0,0" TextAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" />
        <Label Content="Asignar competencia a la pregunta" HorizontalAlignment="Left" Margin="31,22,0,0" VerticalAlignment="Top" Width="323" FontSize="16" FontWeight="Bold"/>
        <Label Content="Competencia:" HorizontalAlignment="Left" Margin="125,132,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.833,0.836"/>

    </Grid>
</Window>
